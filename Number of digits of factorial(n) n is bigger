/* Number of digits of factorial(n)
n is bigger 
*/
#include<stdio.h>
#include<math.h>
int main()
{
    int i,num,count;
    double k;
    while(scanf("%d",&num)==1){
        k=0.0;
        
    for(i=1;i<=num;i++){
        k=k+log(i);
        }
        count=(k/log(10.0))+1;
        printf("%d\n",count);  /* count holo digit er sonkha  */
}
return 0;
}
